{
  "expo": {
    "name": "Lykoo",
    "slug": "Lykoo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lyko.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lyko.png",
        "backgroundColor": "#000000"
      },
      "package": "com.nsrjc.Lykoo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/lyko.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/lyko.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "21e731f9-5872-4c16-b770-078d61c17813"
      }
    },
    "owner": "nsrjc"
  }
}
